{
    "compilerOptions": {
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Node",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "types": ["react/experimental"],
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
